{
  "name": "milist",
  "version": "1.0.4",
  "description": "A simple tool to generate a list of Material Icons available in the Material Icons font",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "milist": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest tests/simple.test.ts tests/cli.test.ts tests/functional.test.ts tests/typescript.test.ts",
    "test:watch": "jest --watch",
    "test:simple": "jest tests/simple.test.ts",
    "test:functional": "jest tests/functional.test.ts",
    "test:cli": "jest tests/cli.test.ts",
    "test:typescript": "jest tests/typescript.test.ts",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "material-icons",
    "icons",
    "material-design",
    "google-icons",
    "cli"
  ],
  "author": "Hamed Zakeri Miyab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hamcker/material-icons-list.git"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.13",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
